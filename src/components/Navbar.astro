---
import Navlink from './Navlink.astro'

const navLinks = [
  { href: '/',         title: 'About Me' },
  { href: '/skills/',   title: 'Skills' },
  { href: '/work/',     title: 'Work' },
  { href: '/projects/', title: 'Projects' },
  { href: '/contact/',  title: 'Contact' },
]
---
<nav>
	<ul>
		{navLinks.map(link => (
      <Navlink href={link.href} title={link.title} />
    ))}
	</ul>
</nav>

